{
  "scripts": {
    "test": "buidler compile && buidler test",
    "compile": "buidler compile",
    "start": "buidler node --port 8565",
    "coverage": "rm -rf cache && buidler compile && buidler coverage --network coverage --temp build && rm -rf cache",
    "gas": "REPORT_GAS=true buidler test --network local",
    "migrate": "./scripts/migrate.js",
    "migrate-local": "./scripts/migrate.js start -n local -a $LOCAL_ADMIN_ADDRESS",
    "migrate-kovan": "./scripts/migrate.js start -n kovan",
    "console-kovan": "oz-console -n kovan",
    "session-kovan": "oz session --network kovan --from $ADMIN_ADDRESS --expires 3600 --timeout 600 --blockTimeout 50",
    "session-local": "oz session --network local --from $LOCAL_ADMIN_ADDRESS --expires 3600 --timeout 600 --blockTimeout 50",
    "clean": "rm -rf build && rm -rf cache",
    "verify-kovan": "oz verify -n kovan --remote etherscan --api-key $ETHERSCAN_API_KEY"
  },
  "devDependencies": {
    "@nomiclabs/buidler": "^1.3.1",
    "@nomiclabs/buidler-ethers": "^1.3.1",
    "@nomiclabs/buidler-etherscan": "^1.3.1",
    "@nomiclabs/buidler-waffle": "^1.3.1",
    "@openzeppelin/cli": "^2.8.1",
    "@truffle/hdwallet-provider": "^1.0.34",
    "@types/chai": "^4.2.10",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.1",
    "buidler-gas-reporter": "^0.1.3",
    "chai": "^4.2.0",
    "chalk": "^4.0.0",
    "deploy-eip-1820": "^0.1.1",
    "ethereum-waffle": "^2.3.2",
    "ethers": "^4.0.45",
    "oz-console": "^2.0.0",
    "shelljs": "0.8.3",
    "solc": "0.6.4",
    "solidity-coverage": "^0.7.4",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^3.0.0-rc.0",
    "@openzeppelin/upgrades": "^2.8.0",
    "@pooltogether/fixed-point": "^0.1.0",
    "@pooltogether/oz-migrate": "^0.2.5",
    "@pooltogether/uniform-random-number": "pooltogether/uniform-random-number#solidity-0.6",
    "sortition-sum-tree-factory": "^0.1.0"
  }
}
